{
	"manifest_version": 2,
	"name": "Chrome Extension Tab",
	"description": "Simple structure for Chrome Extension Creations!",
	"author": "Elvis Canziba",
	"homepage_url": "http://canziba.com",
	"version": "1.0",
	"chrome_url_overrides": {
		"newtab": "chrome_tab.html"
	},
	"icons": {
		"16": "icons/icon16.png",
		"32": "icons/icon32.png",
		"128": "icons/icon128.png"
	},
	"permissions": ["https://source.unsplash.com/", "http://quotesondesign.com/"],
	"content_security_policy": "script-src 'self' https://ajax.googleapis.com; object-src 'self'"
}